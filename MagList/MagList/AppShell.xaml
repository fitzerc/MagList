<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:mainPage="clr-namespace:MagList.MainPage"
    xmlns:listPage="clr-namespace:MagList.ListPage"
    x:Class="MagList.AppShell">
    <TabBar>
        <ShellContent Title="Home"
                     ContentTemplate="{DataTemplate mainPage:MainPage}" />
        <ShellContent Title="Lists"
                     ContentTemplate="{DataTemplate listPage:ListPage}" />
    </TabBar>
</Shell>
